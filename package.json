    "quiz",
    "test"
  ],
  "license": "MIT",
  "main": "lib/server.js",
  "private": true,
  "repository": "nodaguti/word-quiz-generator-webapp",
  "scripts": {
    "build": "npm-run-all clean build:server build:client",
    "build:client": "webpack --config webpack.config.js --progress --display-error-details",
    "build:server": "babel server --out-dir lib --source-maps",
    "clean": "rimraf assets lib",
    "lint": "eslint server/** src/** test/**/*.js",
    "start": "forever start lib/server.js",
    "stop": "forever stop lib/server.js",
    "test": "npm-run-all lint test:server test:e2e",
    "test:e2e": "cross-env RESOURCES=test/fixtures/resources.json npm-run-all test:e2e:prepare test:e2e:run test:e2e:end",
    "test:e2e:end": "npm run stop",
    "test:e2e:prepare": "npm-run-all build start",
    "test:e2e:run": "mocha test/e2e",
    "test:server": "cross-env RESOURCES=test/fixtures/resources.json mocha test/server"
  }
}
